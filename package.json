{
    "name": "react-ui-bb",
    "version": "0.1.2",
    "description": "Template react, typescript, and emotion component library documented with styleguidist",
    "main": "lib/index.js",
    "browser": "lib/index.umd.js",
    "module": "lib/index.es.js",
    "scripts": {
        "build": "rollup -c",
        "docs:build": "styleguidist build",
        "format": "prettier --write 'src/**/*' && eslint --fix  'src/**/*.{js,ts,tsx}'",
        "lint": "eslint src/**/*.tsx",
        "prepublishOnly": "rm -rf lib && npm run build",
        "postbuild": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
        "start": "styleguidist server",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keonik/react-ui-bb.git"
    },
    "keywords": [
        "react",
        "emotion",
        "typescript",
        "component library",
        "building blocks",
        "ui",
        "styleguidist",
        "testing library"
    ],
    "author": "John Fay",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/keonik/react-ui-bb/issues"
    },
    "homepage": "https://github.com/keonik/react-ui-bb#readme",
    "files": [
        "/lib"
    ],
    "peerDependencies": {
        "@emotion/core": "^10.x",
        "@emotion/styled": "^10.x",
        "react": "^16.x",
        "react-dom": "^16.x"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@babel/preset-typescript": "^7.8.3",
        "@emotion/core": "^10.0.27",
        "@emotion/styled": "^10.0.27",
        "@rollup/plugin-typescript": "^3.0.0",
        "@testing-library/react": "^9.4.0",
        "@typescript-eslint/eslint-plugin": "^2.19.0",
        "@typescript-eslint/parser": "^2.20.0",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-module-resolver": "^4.0.0",
        "css-loader": "^3.4.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.18.3",
        "eslint-plugin-react-hooks": "^2.3.0",
        "jest": "^25.1.0",
        "jest-emotion": "^10.0.27",
        "prettier": "^1.19.1",
        "react": "^16.12.0",
        "react-docgen-typescript": "^1.16.2",
        "react-dom": "^16.12.0",
        "react-styleguidist": "^10.6.1",
        "rollup": "^1.31.0",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.2",
        "rollup-plugin-postcss": "^2.1.1",
        "rollup-plugin-root-import": "^1.0.0",
        "rollup-plugin-terser": "^5.2.0",
        "rollup-plugin-typescript2": "^0.26.0",
        "rollup-plugin-uglify": "^6.0.4",
        "style-loader": "^1.1.3",
        "typescript": "^3.7.5",
        "webpack": "^4.41.5"
    },
    "dependencies": {}
}

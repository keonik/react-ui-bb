{
  "name": "react-ui-bb",
  "version": "0.1.2",
  "description": "Template react, typescript, and emotion component library documented with styleguidist",
  "main": "dist/index.js",
  "module": "dist/ui.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdx build",
    "docs:build": "styleguidist build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "prepublishOnly": "rm -rf dist && tsdx build",
    "start": "styleguidist server",
    "test": "jest",
    "test:CI": "jest --coverage --colors --reporters=jest-junit"
  },
  "peerDependencies": {
    "@emotion/core": "^10.x",
    "@emotion/styled": "^10.x",
    "react": "^16.9",
    "react-dom": "^16.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabsWidth": 4,
    "useTabs": false,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keonik/react-ui-bb.git"
  },
  "keywords": [
    "react",
    "emotion",
    "typescript",
    "component library",
    "building blocks",
    "ui",
    "styleguidist",
    "testing library"
  ],
  "author": "John Fay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/keonik/react-ui-bb/issues"
  },
  "homepage": "https://keonik.github.io/react-ui-bb/",
  "devDependencies": {
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "husky": "^4.2.3",
    "jest-emotion": "^10.0.32",
    "jest-junit": "^10.0.0",
    "react": "^16.13.1",
    "react-docgen-typescript": "^1.16.3",
    "react-dom": "^16.13.1",
    "react-styleguidist": "^11.0.3",
    "rollup-plugin-postcss": "^2.5.0",
    "style-loader": "^1.1.3",
    "tsdx": "^0.12.3",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0"
  }
}

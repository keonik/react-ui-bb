{
    "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "components": "./src/components",
                    "utils": "./src/utils",
                    "config": "./src/config"
                },
                "extensions": [".ts", ".tsx"]
            }
        ]
    ]
}
